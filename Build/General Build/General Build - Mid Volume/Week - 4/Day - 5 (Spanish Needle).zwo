        <SteadyState Duration="15.0" PowerLow="0.400" PowerHigh="0.400"/>
        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500"/>
        <SteadyState Duration="15.0" PowerLow="0.400" PowerHigh="0.400"/>
        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500"/>
        <SteadyState Duration="15.0" PowerLow="0.400" PowerHigh="0.400"/>
        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500"/>
        <SteadyState Duration="315.0" PowerLow="0.400" PowerHigh="0.400">
            <textevent message="How do you feel after that first set?" timeoffset="5"/>
            <textevent message="Do you feel like 150% FTP is realistic to hold for subsequent sets?" timeoffset="15"/>
            <textevent message="If need be, drop the watts to 140 or even 130%" timeoffset="25"/>
            <textevent message="Being able to hold your power &amp; cadence for the vast majority of each set..." timeoffset="35"/>
            <textevent message="...is preferable to completing partial sets at the full 150% FTP" timeoffset="45"/>
            <textevent message="So be clear, the goal is to keep your leg speed &amp; power..." timeoffset="55"/>
            <textevent message="for as many microbursts as possible, backpedaling as seldom as possible" timeoffset="65"/>
            <textevent message="Next set coming up in about 30 seconds" timeoffset="285"/>
            <textevent message="Work your spin up to 100rpm or even a little higher..." timeoffset="295"/>
            <textevent message="Probably no more than 120 rpm though" timeoffset="305"/>
        </SteadyState>
        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500">
            <textevent message="Lots of leg speed for 16 total repeats!" timeoffset="0"/>
        </SteadyState>
