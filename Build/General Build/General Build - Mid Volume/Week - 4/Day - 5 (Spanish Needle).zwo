        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500"/>
        <SteadyState Duration="315.0" PowerLow="0.400" PowerHigh="0.400">
            <textevent message="Nice! Keep those pedals turning, forward or backward works" timeoffset="0"/>
            <textevent message="If you can keep the pedals turning forward immediately after a hard set..." timeoffset="10"/>
            <textevent message="you'll help your legs clear more quickly, recover a bit faster" timeoffset="20"/>
            <textevent message="Feel free to take a minute to stretch, maybe shake those legs out" timeoffset="135"/>
            <textevent message="It's important during the workout as a whole..." timeoffset="285"/>
            <textevent message="and during the sets of microbursts to keep yourself present" timeoffset="295"/>
            <textevent message="No sense in discouraging yourself by getting ahead of things" timeoffset="305"/>
        </SteadyState>
        <SteadyState Duration="15.0" PowerLow="1.500" PowerHigh="1.500">
            <textevent message="Take these 1 at a time, as though each microburst is the only one" timeoffset="0"/>
        </SteadyState>
