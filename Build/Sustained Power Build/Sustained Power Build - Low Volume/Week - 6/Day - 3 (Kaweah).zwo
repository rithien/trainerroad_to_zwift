            <textevent message="First interval's in 1 minute..." timeoffset="240.000000"/>
            <textevent message="so set your cadence before the work begins" timeoffset="244.000000"/>
            <textevent message="If possible, ride at 85rpm or higher" timeoffset="251.000000"/>
            <textevent message="If this cadence is too quick for you..." timeoffset="257.000000"/>
            <textevent message="find something sustainable..." timeoffset="261.000000"/>
