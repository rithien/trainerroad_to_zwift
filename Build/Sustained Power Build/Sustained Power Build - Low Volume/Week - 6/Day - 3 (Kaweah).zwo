            <textevent message="Lightly kick, pull, then lift..." timeoffset="270.000000"/>
            <textevent message="kick, pull, lift" timeoffset="275.000000"/>
            <textevent message="Try to keep this going for another minute or so" timeoffset="280.000000"/>
            <textevent message="Well done" timeoffset="350.000000"/>
            <textevent message="Now give equal attention to both legs again" timeoffset="355.000000"/>
            <textevent message="That drill was called Single Leg Focus..." timeoffset="361.000000"/>
            <textevent message="and is similar to ILT (one-leggers)..." timeoffset="366.000000"/>
            <textevent message="except both feet stay connected" timeoffset="371.000000"/>
            <textevent message="Not only is it a useful Form drill..." timeoffset="376.000000"/>
            <textevent message="but it's a great distraction from the discomfort..." timeoffset="381.000000"/>
            <textevent message="that often accompanies interval training" timeoffset="385.000000"/>
