            <textevent message="where your legs flood excessively..." timeoffset="83.000000"/>
            <textevent message="your muscles will learn to process higher levels of lactate..." timeoffset="87.000000"/>
            <textevent message="allowing you to work..." timeoffset="93.000000"/>
            <textevent message="for longer stretches of time at this intensity" timeoffset="97.000000"/>
            <textevent message="Put another way..." timeoffset="103.000000"/>
            <textevent message="you'll grow your muscular endurance" timeoffset="107.000000"/>
            <textevent message="Work like this also paves the way..." timeoffset="113.000000"/>
            <textevent message="for higher sustainable power" timeoffset="117.000000"/>
