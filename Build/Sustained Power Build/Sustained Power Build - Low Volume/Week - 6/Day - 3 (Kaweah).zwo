            <textevent message="Slow your spin &amp; get ready" timeoffset="236.000000"/>
            <textevent message="Body up, hips forward" timeoffset="240.000000"/>
            <textevent message="Feel any better?" timeoffset="246.000000"/>
            <textevent message="A bit more controlled?" timeoffset="250.000000"/>
            <textevent message="We're not expecting immediate perfection..." timeoffset="255.000000"/>
