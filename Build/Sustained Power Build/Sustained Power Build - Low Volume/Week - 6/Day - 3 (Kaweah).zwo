            <textevent message="to knee a very light ball..." timeoffset="443.000000"/>
            <textevent message="as it does to activate your hip flexors in this manner" timeoffset="447.000000"/>
            <textevent message="Just focus on lightly lifting your knees..." timeoffset="454.000000"/>
            <textevent message="for 20 more seconds" timeoffset="459.000000"/>
            <textevent message="Excellent" timeoffset="480.000000"/>
            <textevent message="Now let's put all of that together..." timeoffset="484.000000"/>
            <textevent message="for a couple minutes" timeoffset="489.000000"/>
            <textevent message="Both legs, opposite tasks..." timeoffset="494.000000"/>
            <textevent message="fluid as can be" timeoffset="499.000000"/>
            <textevent message="One kicks while the other pulls..." timeoffset="504.000000"/>
            <textevent message="and one lifts while the other presses down" timeoffset="509.000000"/>
            <textevent message="One set of muscles relaxes..." timeoffset="515.000000"/>
            <textevent message="while the other set works" timeoffset="519.000000"/>
            <textevent message="One more minute of seamlessly blending..." timeoffset="540.000000"/>
            <textevent message="all four quadrants of each leg's pedal revolutions" timeoffset="545.000000"/>
            <textevent message="As you do so..." timeoffset="551.000000"/>
            <textevent message="keep your sit-bones anchored..." timeoffset="555.000000"/>
            <textevent message="and your upper body relaxed" timeoffset="559.000000"/>
        </SteadyState>
        <SteadyState Duration="300.0" PowerLow="0.400" PowerHigh="0.400">
