            <textevent message="over the rest of this Threshold interval" timeoffset="156.000000"/>
            <textevent message="Gradually slow your spin..." timeoffset="176.000000"/>
            <textevent message="and come up for 5 seconds" timeoffset="180.000000"/>
            <textevent message="Better?" timeoffset="186.000000"/>
            <textevent message="Let's talk about how to smooth these out" timeoffset="190.000000"/>
