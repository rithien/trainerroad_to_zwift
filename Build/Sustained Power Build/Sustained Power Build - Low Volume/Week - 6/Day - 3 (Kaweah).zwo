            <textevent message="but feel free to spin a little more slowly..." timeoffset="17.000000"/>
            <textevent message="if standing while pedaling is new territory for you" timeoffset="23.000000"/>
            <textevent message="And spin above 70rpm if it suits you..." timeoffset="29.000000"/>
            <textevent message="and your particularly practiced standing pedaling style" timeoffset="34.000000"/>
            <textevent message="Roughly every minute on the minute..." timeoffset="60.000000"/>
            <textevent message="let's try getting out of the saddle for 5 seconds" timeoffset="65.000000"/>
            <textevent message="If you're on an electronic trainer in Erg mode..." timeoffset="72.000000"/>
            <textevent message="you'll simply slow your spin prior to standing" timeoffset="77.000000"/>
            <textevent message="And if you're on a regular trainer..." timeoffset="83.000000"/>
            <textevent message="you'll need to shift down 1 or 2 gears" timeoffset="86.000000"/>
            <textevent message="In either case..." timeoffset="93.000000"/>
            <textevent message="don't plan on standing at 85rpm or higher..." timeoffset="96.000000"/>
            <textevent message="unless you're very practiced &amp; comfortable..." timeoffset="102.000000"/>
            <textevent message="with high-cadence, out-of-the-saddle pedaling" timeoffset="107.000000"/>
            <textevent message="Gently slow things down..." timeoffset="113.000000"/>
            <textevent message="to maybe 60-70rpm..." timeoffset="117.000000"/>
            <textevent message="and rise for 5 seconds" timeoffset="121.000000"/>
            <textevent message="How'd that go?" timeoffset="127.000000"/>
            <textevent message="Great? Horribly?" timeoffset="131.000000"/>
            <textevent message="Saddle transitioning is a skill every rider can improve" timeoffset="136.000000"/>
