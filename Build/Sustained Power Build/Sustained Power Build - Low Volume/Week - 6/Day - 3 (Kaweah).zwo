            <textevent message="Job done!" timeoffset="0"/>
            <textevent message="Job WELL done!" timeoffset="5"/>
            <textevent message="You just accumulated 50 minutes..." timeoffset="10"/>
            <textevent message="of near-FTP work" timeoffset="14"/>
            <textevent message="And it didn't crush you..." timeoffset="19"/>
            <textevent message="like a 50-minute time trial would have" timeoffset="23"/>
            <textevent message="What's more..." timeoffset="30"/>
            <textevent message="you learned a few things you can apply to future rides..." timeoffset="34"/>
            <textevent message="indoors or out" timeoffset="40"/>
            <textevent message="Get a jump on your post-workout nutrition..." timeoffset="45"/>
            <textevent message="and until next time..." timeoffset="51"/>
            <textevent message="train smart, ride fast, have fun" timeoffset="55"/>
