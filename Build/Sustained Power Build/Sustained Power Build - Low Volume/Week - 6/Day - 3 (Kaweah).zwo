            <textevent message="by gently lifting your right knee..." timeoffset="139.000000"/>
            <textevent message="just after pulling back your heel" timeoffset="144.000000"/>
            <textevent message="This is similar to juggling a soccer ball..." timeoffset="160.000000"/>
