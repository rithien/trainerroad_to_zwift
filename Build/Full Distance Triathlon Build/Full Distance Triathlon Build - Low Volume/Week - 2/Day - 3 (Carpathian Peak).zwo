            <textevent message="the ones that get tailed off the back..." timeoffset="200.000000"/>
            <textevent message="when the pace jumps up or the road tips upward..." timeoffset="205.000000"/>
            <textevent message="only to gradually claw their way back on..." timeoffset="210.000000"/>
            <textevent message="over and over again" timeoffset="215.000000"/>
