            <textevent message="This ability to go hard..." timeoffset="176.000000"/>
            <textevent message="then harder, then hard again..." timeoffset="180.000000"/>
            <textevent message="creates that diesel power..." timeoffset="185.000000"/>
            <textevent message="you see in the most stubbornly unshakeable, unbreakable riders" timeoffset="189.000000"/>
