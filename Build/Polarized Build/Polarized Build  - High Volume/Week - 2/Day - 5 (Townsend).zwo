            <textevent message="and your power output, cadence, and form..." timeoffset="25"/>
            <textevent message="can all see unfavorable declines" timeoffset="30"/>
            <textevent message="Easily remedied though!" timeoffset="35"/>
            <textevent message="Simply treat each remaining segment as its own interval..." timeoffset="40"/>
