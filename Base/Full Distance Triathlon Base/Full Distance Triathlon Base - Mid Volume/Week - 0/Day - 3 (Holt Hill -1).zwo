            <textevent message="your emphasis shifts to the 10 o'clock to 2 o'clock segment..." timeoffset="66.000000"/>
            <textevent message="of each pedal revolution" timeoffset="71.000000"/>
            <textevent message="And much like your pelvic tilt that anchors your sit-bones..." timeoffset="76.000000"/>
            <textevent message="(subtle reminder)" timeoffset="82.000000"/>
            <textevent message="this doesn't require much energy at all" timeoffset="86.000000"/>
            <textevent message="When you consider how many times you turn the pedals each minute..." timeoffset="92.000000"/>
