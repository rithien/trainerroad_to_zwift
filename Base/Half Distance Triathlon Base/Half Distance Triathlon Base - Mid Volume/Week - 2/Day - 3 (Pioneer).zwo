            <textevent message="when a less comfortable but more powerful position is appropriate" timeoffset="172.000000"/>
            <textevent message="But..." timeoffset="179.000000"/>
            <textevent message="SLIDING foward is not the same as TIPPING foward" timeoffset="182.000000"/>
