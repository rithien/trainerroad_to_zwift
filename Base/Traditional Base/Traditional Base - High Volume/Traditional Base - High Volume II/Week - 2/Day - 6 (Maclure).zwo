            <textevent message="imagine how steady &amp; controlled you'll be when both legs are working" timeoffset="638.000000"/>
            <textevent message="Final minute in this round..." timeoffset="660.000000"/>
            <textevent message="and time to once again check your grip" timeoffset="664.000000"/>
            <textevent message="Third round behind you, both feet back in the pedals" timeoffset="720.000000"/>
            <textevent message="Try to spin off any fatigue in that recently working leg..." timeoffset="727.000000"/>
            <textevent message="and decide if one more round will be beneficial..." timeoffset="733.000000"/>
            <textevent message="or maybe asking a little too much of tired muscles" timeoffset="739.000000"/>
            <textevent message="Get ready for the final round..." timeoffset="770.000000"/>
            <textevent message="as long as you can keep it pretty" timeoffset="774.000000"/>
            <textevent message="One foot out, power on Target" timeoffset="780.000000"/>
            <textevent message="Only 2 minutes to go..." timeoffset="840.000000"/>
            <textevent message="but there's no shame in skipping the remaining drills..." timeoffset="844.000000"/>
            <textevent message="if you suspect your form will suffer" timeoffset="849.000000"/>
