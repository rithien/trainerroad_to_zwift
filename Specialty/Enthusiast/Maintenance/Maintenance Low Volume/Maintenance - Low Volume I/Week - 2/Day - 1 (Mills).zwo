            <textevent message="for longer than 3 minutes..." timeoffset="64"/>
            <textevent message="at least not on a repeat basis" timeoffset="69"/>
            <textevent message="So having done a single 2-minute interval starting at 120% FTP..." timeoffset="75"/>
