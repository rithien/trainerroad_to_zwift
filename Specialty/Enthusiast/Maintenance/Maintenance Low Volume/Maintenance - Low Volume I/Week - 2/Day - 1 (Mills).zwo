            <textevent message="Try relaxing your muscles..." timeoffset="7"/>
            <textevent message="upper &amp; lower body..." timeoffset="11"/>
            <textevent message="and continuing those belly-breaths" timeoffset="15"/>
            <textevent message="As mentioned earlier..." timeoffset="90"/>
            <textevent message="the main objective is improved aerobic capacity" timeoffset="94"/>
            <textevent message="Without getting too scientific or detailed..." timeoffset="100"/>
            <textevent message="we're looking to improve the amount of work..." timeoffset="105"/>
            <textevent message="that you can do at peak oxygen uptake" timeoffset="110"/>
