            <textevent message="It's not going to feel &quot;better&quot; exactly..." timeoffset="12"/>
            <textevent message="but it should keep you hanging in there..." timeoffset="17"/>
            <textevent message="especially if you keep a quick spin" timeoffset="22"/>
            <textevent message="Keep your Power on Target for 30 more seconds..." timeoffset="45"/>
            <textevent message="with a quick, fluid spin..." timeoffset="51"/>
            <textevent message="and lots of complete breaths" timeoffset="55"/>
            <textevent message="10 seconds to go, don't ease up!" timeoffset="65"/>
            <textevent message="3 seconds!" timeoffset="72"/>
        </Cooldown>
        <SteadyState Duration="180.0" PowerLow="0.400" PowerHigh="0.400">
