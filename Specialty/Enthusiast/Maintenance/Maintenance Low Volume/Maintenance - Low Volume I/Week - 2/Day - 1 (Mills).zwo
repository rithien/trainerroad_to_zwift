<?xml version="1.0" ?>
<workout_file>
    <description>Mills consists of 3 sets of 3x2-minute VO2max repeats each starting at 120% FTP and finishing at 110% FTP.3-minute recoveries fall between intervals and 6-minute recoveries separate the sets.</description>
    <name>Mills</name>
    <workout>
        <SteadyState Duration="60.0" PowerLow="0.500" PowerHigh="0.500">
            <textevent message="Welcome to Mills..." timeoffset="10"/>
            <textevent message="where &quot;short &amp; sweet&quot; is the name of the game" timeoffset="16"/>
