            <textevent message="that you're in the midst of right now" timeoffset="139"/>
            <textevent message="You're not JUST getting good at working at this intensity..." timeoffset="145"/>
            <textevent message="you're also paving the way for later adaptations..." timeoffset="151"/>
            <textevent message="that can increase your overall aerobic capacity..." timeoffset="156"/>
