            <textevent message="Alternatively..." timeoffset="72"/>
            <textevent message="as you increase your pedaling speed..." timeoffset="75"/>
            <textevent message="and make more crank revolutions per minute..." timeoffset="80"/>
            <textevent message="you lift the circumferential velocity..." timeoffset="85"/>
            <textevent message="or more simply, leg SPEED" timeoffset="89"/>
            <textevent message="In endurance parlance..." timeoffset="94"/>
