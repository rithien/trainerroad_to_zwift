            <textevent message="ideally somewhere above 85rpm..." timeoffset="6"/>
            <textevent message="but probably no higher than 105rpm" timeoffset="11"/>
            <textevent message="Time to sprinkle in some efforts more specific..." timeoffset="90"/>
            <textevent message="to the demands of today's main set" timeoffset="96"/>
            <textevent message="Do your best to maintain that quick spin..." timeoffset="102"/>
            <textevent message="maybe even lift it a little if you can control it" timeoffset="107"/>
        </Cooldown>
        <SteadyState Duration="30.0" PowerLow="1.150" PowerHigh="1.150">
            <textevent message="Quick legs but keep your upper body relaxed..." timeoffset="0"/>
            <textevent message="and glue your sit-bones to your seat..." timeoffset="6"/>
            <textevent message="with even pressure on each wing of the saddle" timeoffset="11"/>
        </SteadyState>
        <SteadyState Duration="60.0" PowerLow="0.400" PowerHigh="0.400">
            <textevent message="Take it easy for a minute..." timeoffset="0"/>
            <textevent message="but keep your legs comfortably quick" timeoffset="5"/>
            <textevent message="Bring your cadence up a little..." timeoffset="50"/>
            <textevent message="and get ready to go again" timeoffset="55"/>
        </SteadyState>
        <SteadyState Duration="30.0" PowerLow="1.200" PowerHigh="1.200">
            <textevent message="Consider a slightly quicker spin than last time..." timeoffset="0"/>
            <textevent message="and start to get in touch..." timeoffset="6"/>
            <textevent message="with some deep, belly-driven breathing" timeoffset="10"/>
        </SteadyState>
        <SteadyState Duration="60.0" PowerLow="0.400" PowerHigh="0.400">
            <textevent message="Ease off the power..." timeoffset="0"/>
            <textevent message="but keep most of the leg speed" timeoffset="4"/>
            <textevent message="Get ready for one more..." timeoffset="44"/>
            <textevent message="and your quickest legs yet..." timeoffset="48"/>
            <textevent message="as long as there's no bouncing in the saddle" timeoffset="52"/>
        </SteadyState>
        <SteadyState Duration="30.0" PowerLow="1.250" PowerHigh="1.250">
