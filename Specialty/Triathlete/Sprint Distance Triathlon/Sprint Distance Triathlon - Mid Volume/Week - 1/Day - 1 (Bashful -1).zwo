<?xml version="1.0" ?>
<workout_file>
    <description>Bashful -1 is 3 sets of 5x1-minute intervals at 125% FTP where recoveries between intervals are 2 minutes long and recoveries between sets of intervals are 3 minutes long.</description>
    <name>Bashful -1</name>
    <workout>
        <Warmup Duration="120.0" PowerLow="0.500" PowerHigh="0.520">
            <textevent message="Welcome to Bashful -1" timeoffset="10"/>
            <textevent message="this is a straightforward foray into VO2max work" timeoffset="21"/>
