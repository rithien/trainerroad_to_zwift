            <textevent message="No reason you can't make this your best Form Sprint yet..." timeoffset="547.000000"/>
            <textevent message="outside of the fact that you're pretty tired by now" timeoffset="553.000000"/>
            <textevent message="So use good judgement in determining how/if you'll sprint" timeoffset="560.000000"/>
            <textevent message="A little tired? Try for the out-of-the-saddle jump" timeoffset="567.000000"/>
            <textevent message="More tired? Perform the entire sprint seated" timeoffset="574.000000"/>
            <textevent message="Really tired? Skip the sprint entirely" timeoffset="581.000000"/>
            <textevent message="In 10!" timeoffset="590.000000"/>
            <textevent message="5..." timeoffset="595.000000"/>
            <textevent message="4..." timeoffset="596.000000"/>
            <textevent message="3..." timeoffset="597.000000"/>
            <textevent message="2..." timeoffset="598.000000"/>
            <textevent message="1..." timeoffset="599.000000"/>
            <textevent message="Sprint!" timeoffset="600.000000"/>
            <textevent message="5 seconds!" timeoffset="605.000000"/>
            <textevent message="Back it off" timeoffset="610.000000"/>
            <textevent message="Take it easy for a few minutes and calm your breathing" timeoffset="620.000000"/>
