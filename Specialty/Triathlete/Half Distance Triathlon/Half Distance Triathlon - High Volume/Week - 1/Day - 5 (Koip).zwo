            <textevent message="and calming any chatter in your hips..." timeoffset="324.000000"/>
            <textevent message="by relaxing your lower legs" timeoffset="328.000000"/>
            <textevent message="Another minute of quicker-than-usual legs" timeoffset="360.000000"/>
            <textevent message="Final minute of that slightly elevated spin" timeoffset="420.000000"/>
            <textevent message="Take it easy for a couple minutes..." timeoffset="480.000000"/>
            <textevent message="and then we'll talk about that next Form Sprint" timeoffset="486.000000"/>
