            <textevent message="and patiently wait for the heaviness to clear" timeoffset="5"/>
            <textevent message="Even though your fatigue is at its highest point..." timeoffset="12"/>
            <textevent message="you've survived several of these valleys already" timeoffset="18"/>
            <textevent message="What's one more? ;)" timeoffset="25"/>
        </SteadyState>
        <Warmup Duration="60.0" PowerLow="0.950" PowerHigh="1.100">
