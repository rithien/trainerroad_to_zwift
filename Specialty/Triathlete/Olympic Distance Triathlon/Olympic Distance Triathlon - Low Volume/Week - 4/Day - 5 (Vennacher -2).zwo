            <textevent message="as you slowly ramp your Power back up" timeoffset="5"/>
            <textevent message="Sit-bones down, hips quiet..." timeoffset="11"/>
            <textevent message="full, deep breaths driven by your belly" timeoffset="15"/>
        </Warmup>
        <Cooldown Duration="60.0" PowerLow="1.100" PowerHigh="0.950">
            <textevent message="One more gradual descent in effort..." timeoffset="0"/>
            <textevent message="but not one that offers quick relief" timeoffset="5"/>
            <textevent message="So be patient, be tough..." timeoffset="11"/>
