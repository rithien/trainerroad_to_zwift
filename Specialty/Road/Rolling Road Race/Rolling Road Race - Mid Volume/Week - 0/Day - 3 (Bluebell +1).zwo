            <textevent message="Do your best to maintain that quick spin..." timeoffset="162"/>
            <textevent message="maybe even lift it a little if you can control it" timeoffset="167"/>
        </SteadyState>
        <SteadyState Duration="20.0" PowerLow="1.200" PowerHigh="1.200">
