            <textevent message="without changing gears or your cadence" timeoffset="26.000000"/>
            <textevent message="We'll limit these &quot;shifts&quot; to the 30-second surges..." timeoffset="32.000000"/>
            <textevent message="so until then..." timeoffset="38.000000"/>
            <textevent message="just anchor your sit-bones..." timeoffset="42.000000"/>
