            <textevent message="such that your sit-bones press down on your saddle" timeoffset="70.000000"/>
            <textevent message="Almost time to give this position change a shot" timeoffset="90.000000"/>
            <textevent message="Try to maintain your anchored, pelvic position..." timeoffset="97.000000"/>
