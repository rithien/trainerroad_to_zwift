            <textevent message="these efforts are HIGHLY aerobic..." timeoffset="14.000000"/>
            <textevent message="lots of oxygen being utilized" timeoffset="21.000000"/>
            <textevent message="They'll get increasingly aerobic as the set continues to" timeoffset="28.000000"/>
