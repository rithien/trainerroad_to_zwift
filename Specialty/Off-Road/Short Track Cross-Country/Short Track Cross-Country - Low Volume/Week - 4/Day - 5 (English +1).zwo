            <textevent message="Backpedal briefly or spin lightly" timeoffset="7.000000"/>
            <textevent message="Quick spin, forward if you were backpedaling" timeoffset="30.000000"/>
        </SteadyState>
        <SteadyState Duration="60.0" PowerLow="1.270" PowerHigh="1.270">
